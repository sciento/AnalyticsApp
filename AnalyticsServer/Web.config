    <standardEndpoints>
      <webHttpEndpoint>
        <standardEndpoint name="" helpEnabled="true" automaticFormatSelectionEnabled="false" />
      </webHttpEndpoint>
    </standardEndpoints>
    <behaviors>
      <endpointBehaviors>
        <behavior></behavior>
      </endpointBehaviors>
      <serviceBehaviors>
        <behavior name="AnalyticsServer_Behavior">
